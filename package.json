{
  "name": "git-git-git",
  "version": "1.0.3",
  "description": "A CLI tool for managing your git branches",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "ggg": "./dist/index.js"
  },
  "scripts": {
    "watch": "npm run clean && DEV=true babel ./src -d ./dist -w",
    "build": "npm run clean && babel ./src -d ./dist",
    "clean": "rimraf ./dist",
    "lint": "eslint '**/*.js' --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalares/git-git-git"
  },
  "keywords": [
    "git",
    "git-git-git",
    "git cli tool"
  ],
  "author": "Christian Alares",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.13.17",
    "chalk": "^4.1.1",
    "ink": "^3.0.8",
    "ink-gradient": "^2.0.0",
    "ink-select-input": "^4.2.0",
    "ink-text-input": "^4.0.1",
    "react": "^17.0.2",
    "simple-git": "^2.38.0",
    "zustand": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
